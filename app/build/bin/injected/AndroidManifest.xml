<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.mhook.MrSFastTranslation"
	android:versionCode="50"
	android:versionName="2.8.2-debug">

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="21"/>

	<application
		android:label="Xp快译"
		android:icon="@drawable/icon"
		android:name=".MyApplication">

		<activity
			android:windowSoftInputMode="adjustUnspecified|stateHidden"
			android:name=".SettingActivity">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="de.robv.android.xposed.category.MODULE_SETTINGS"/>

			</intent-filter>

		</activity>

		<activity-alias
			android:name=".SettingActivity-alias"
			android:enabled="true"
			android:targetActivity=".SettingActivity">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity-alias>

		<meta-data
			android:name="xposedmodule"
			android:value="true"/>

		<meta-data
			android:name="xposeddescription"
			android:value="一个简单的翻译小模块."/>

		<meta-data
			android:name="xposedminversion"
			android:value="30"/>

	</application>

	<uses-permission
		android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

	<uses-permission
		android:name="android.permission.INTERNET"/>

	<uses-permission
		android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

</manifest>

